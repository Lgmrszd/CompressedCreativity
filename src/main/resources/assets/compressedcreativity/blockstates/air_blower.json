{
  "multipart": [
    { "when": {"facing": "north" }, "apply": { "model": "compressedcreativity:block/air_blower"}},
    { "when": {"facing": "south" }, "apply": { "model": "compressedcreativity:block/air_blower", "y": 180 }},
    { "when": {"facing": "west" },  "apply": { "model": "compressedcreativity:block/air_blower", "y": 270 }},
    { "when": {"facing": "east" },  "apply": { "model": "compressedcreativity:block/air_blower", "y": 90 }},
    { "when": {"facing": "up"   },  "apply": { "model": "compressedcreativity:block/air_blower", "x": 270}},
    { "when": {"facing": "down" },  "apply": { "model": "compressedcreativity:block/air_blower", "x": 90 }},
    { "when": {"down": true },  "apply": { "model": "compressedcreativity:block/air_blower_part", "x": 180 }},
    { "when": {"up": true },    "apply": { "model": "compressedcreativity:block/air_blower_part" }},
    { "when": {"north": true }, "apply": { "model": "compressedcreativity:block/air_blower_part", "x": 90 }},
    { "when": {"south": true }, "apply": { "model": "compressedcreativity:block/air_blower_part", "x": 90, "y": 180 }},
    { "when": {"west": true },  "apply": { "model": "compressedcreativity:block/air_blower_part", "x": 90, "y": 270 }},
    { "when": {"east": true },  "apply": { "model": "compressedcreativity:block/air_blower_part", "x": 90, "y": 90 }}
  ]
}